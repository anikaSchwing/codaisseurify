<h4>Add a new song</h4>
<%= form_for [@artist, @song] do |f| %>
    <div class="row">
    <%= f.label :title, class:"col-form-label col-md-1" %>
    <div class="col-md-8">
      <%= f.text_field :title, class:"form-control" %>
    </div>
      <%= f.submit "Create", class:"btn btn-success" %>
    </div>
<% end %>
